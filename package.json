{
  "name": "teamproject",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "node": "node",
    "postinstall": "npm run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "rimraf": "rimraf",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "echo \"Error: no test specified\"",
    "tslint": "tslint",
    "version": "npm run build",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JavaScript-Jedis/team-project.git"
  },
  "contributors": [
    "James Fullmer",
    "Preston Haynes",
    "Mike Hodges",
    "Doug Irwin <dirwin2@bruinmail.slcc.edu> (http://dougirw.in)"
  ],
  "bugs": {
    "url": "https://github.com/JavaScript-Jedis/team-project/issues"
  },
  "homepage": "https://github.com/JavaScript-Jedis/team-project#readme",
  "devDependencies": {
    "@ngtools/webpack": "1.10.0",
    "@types/node": "^9.6.1",
    "@types/uglify-js": "^2.6.30",
    "@types/webpack": "^3.8.8",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^2.30.1",
    "ngc-webpack": "^4.1.2",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.20.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "^5.0.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-dev-server": "~2.11.1",
    "webpack-inline-manifest-plugin": "4.0.0",
    "webpack-merge": "~4.1.2"
  }
}
